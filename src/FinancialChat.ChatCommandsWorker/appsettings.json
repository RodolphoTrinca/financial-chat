{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "ApplicationName": "Financial Chat API"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=db; Database=financialmanagement; Username=postgres; Password=admin"
  },
  "RabbitMQConnectionSettings": {
    "HostName": "localhost",
    "Username": "guest",
    "Password": "guest"
  },
  "RabbitMQQueueNames": {
    "StockPriceRequest": "stockPriceRequest",
    "ChatMessages": "chatMessages",
    "HubConnectionMessages": "hubConnectionMessages"
  },
  "StockPriceConfig": {
    "Url": "https://stooq.com/",
    "Path": "q/l/",
    "Format": "csv",
    "F": "sd2t2ohlcv"
  }
}
